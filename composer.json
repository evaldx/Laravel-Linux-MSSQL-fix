    "name": "evaldx/laravel-linux-mssql-fix",
    "description": "An optimized SQL Server Grammar for Laravel",
    "keywords": ["sqlsrv", "laravel", "custom", "sqlserver", "sql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evalds Meskovskis",
            "email": "xxxevaldxxx@inbox.lv"
        }
    ],
    "support": {
        "issues": "https://github.com/evaldx/Laravel-Linux-MSSQL-fix/issues",
